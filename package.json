{
  "name": "@akmf/ksef-fe-invoice-converter",
  "version": "0.0.30",
  "scripts": {
    "dev": "vite --mode public --config vite.config.ts",
    "build": "vite build --mode production",
    "build:cli": "bun build src/cli.ts --compile --outfile ksef-pdf",
    "build:cli:linux": "bun build src/cli.ts --compile --target=bun-linux-x64 --outfile ksef-pdf-linux",
    "build:cli:windows": "bun build src/cli.ts --compile --target=bun-windows-x64 --outfile ksef-pdf.exe",
    "build:cli:macos": "bun build src/cli.ts --compile --target=bun-darwin-x64 --outfile ksef-pdf-macos",
    "build:cli:all": "bun run build:cli:linux && bun run build:cli:windows && bun run build:cli:macos",
    "cli": "bun run src/cli.ts",
    "type": "tsc -p src/app-public/tsconfig.json --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage"
  },
  "main": "./dist/ksef-fe-invoice-converter.umd.cjs",
  "module": "./dist/ksef-fe-invoice-converter.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ksef-fe-invoice-converter.js",
      "require": "./dist/ksef-fe-invoice-converter.umd.cjs"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.50.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/pdfmake": "^0.2.11",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "pdfmake": "^0.2.20",
    "xml-js": "^1.6.11"
  }
}
